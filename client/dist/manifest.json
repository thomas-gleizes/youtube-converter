{
  "name": "youtube converter mp3",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "convertir la video que tu regarde en mp3 directement",
  "icons": {
    "512": "favicon.png"
  },
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": ["app.js"]
  }],
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "webNavigation", "activeTab", "tabs"]
}